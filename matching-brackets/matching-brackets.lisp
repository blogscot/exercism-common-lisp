(defpackage :matching-brackets  (:use :cl)  (:export :pairedp))(in-package :matching-brackets)(defun pairedp (brackets)  (null (reduce (lambda (acc bracket)                  (case bracket                    (#\( (cons #\) acc))                    (#\{ (cons #\} acc))                    (#\[ (cons #\] acc))                    ((#\) #\} #\]) (if (eql bracket (car acc))                                       (cdr acc)                                       (cons 'error acc)))                    (otherwise acc)))                brackets :initial-value '())))